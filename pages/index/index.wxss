.swiper-container {
  position: relative;
}

.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 50rpx;
  display: flex;
  justify-content: center;
}

.swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 8rpx;
  transition: all 0.6s;
}

.swiper-container .dots .dot.active {
  width: 24rpx;
  background: #5677fc;
}

.four-menu {
  width: 700rpx;
  height:400rpx;
  margin: 0 auto;
  overflow: hidden;
}

.four-menu .item {
  display: block;
  width: 330rpx;
  height: 156rpx;
  border-radius: 10rpx;
  background-color: #fff;
  float: left;
  margin-top: 30rpx;
