.new-hot {
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 20rpx;
  color: #939393;
  background-color: #fff;
  /* margin-top: 30rpx; */
  position: relative;
  border-bottom: 1rpx solid #f1f1f1;
}

.new-hot text {
  position: relative;
  display: inline-block;
}

.new-hot text::before {
  content: '';
  display: block;
  width: 30rpx;
  height: 1rpx;
  background-color: #e5e5e5;
  position: absolute;
  left: -40rpx;
  top: 28rpx;
}

.new-hot text::after {
  content: '';
  display: block;
  width: 30rpx;
  height: 1rpx;
  background-color: #e5e5e5;
  position: absolute;
  right: -40rpx;
  top: 28rpx;
}

