.ceng {
		width: 100%;
		height: 100%;
		position: fixed;
		left: 0;
		top: 0;
		z-index: 300;
		background-color: rgba(0, 0, 0, 0.5);
	}

	.rulebox {
		width: 600rpx;
		background-color: #fff;
		border-radius: 20rpx;
		position: fixed;
		z-index: 350;
		left: 50%;
		margin-left: -300rpx;
		top: 50%;
		-webkit-transform: translateY(-50%);
		transform: translateY(-50%);
		font-size: 32rpx;
	}

	.rulebox .hd {
		width: 100%;
		height: 80rpx;
		text-align: center;
		line-height: 80rpx;
		background-color: #0F9BEC;
		color: #fff;
		border-radius: 20rpx 20rpx 0 0;
	}

	.rulebox .bd {
		padding: 30rpx;
	}

	.rulebox .bd view {
		margin-bottom: 20rpx;
	}

	.rulebox .bd .runman {
		width: 100%;
		display: block;
	}

	.rulebox .close {
		width: 400rpx;
		height: 70rpx;
		text-align: center;
		line-height: 70rpx;
		background-color: #0F9BEC;
		color: #fff;
		margin: 0 auto;
		margin-top: 50rpx;
		margin-bottom: 30rpx;
		border-radius: 6rpx;
		font-size: 28rpx;
	}




